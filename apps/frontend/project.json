{
    "name": "frontend",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/frontend/src",
    "targets": {
        "dev": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run dev",
                "cwd": "apps/frontend"
            }
        },
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run build",
                "cwd": "apps/frontend"
            }
        },
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm start",
                "cwd": "apps/frontend"
            }
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm test",
                "cwd": "apps/frontend"
            }
        },
        "lint": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run lint",
                "cwd": "apps/frontend"
            }
        }
    },
    "tags": [
        "type:app",
        "scope:frontend"
    ]
}