{
    "name": "backend",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/backend/src",
    "targets": {
        "dev": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run dev",
                "cwd": "apps/backend"
            }
        },
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run build",
                "cwd": "apps/backend"
            }
        },
        "start": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm start",
                "cwd": "apps/backend"
            }
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm test",
                "cwd": "apps/backend"
            }
        },
        "lint": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run lint",
                "cwd": "apps/backend"
            }
        }
    },
    "tags": [
        "type:app",
        "scope:backend"
    ]
}